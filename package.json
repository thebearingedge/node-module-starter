{
  "name": "node-module-starter",
  "private": true,
  "version": "0.0.0",
  "description": "JavaScript module boilerplate with testing",
  "main": "index.js",
  "scripts": {
    "istanbul": "babel-node node_modules/.bin/babel-istanbul",
    "lint": "eslint src/*.js && eslint test/*.js",
    "test": "mocha --compilers js:babel-register -r babel-polyfill",
    "tdd": "npm test -s -- -w -R min",
    "cover": "npm run -s istanbul -- cover _mocha --",
    "babel": "rimraf dist && babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "@thebearingedge/test-utils": "3.3.0",
    "babel-cli": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-istanbul": "0.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.9.0",
    "eslint": "2.10.2",
    "mocha": "2.4.5",
    "rimraf": "2.5.2"
  },
  "dependencies": {
    "babel-polyfill": "6.9.0"
  }
}
